# shellcheck disable=SC2206

export GOPATH=$HOME/go

# Set the list of directories that Zsh searches for programs.
path=(
  ./bin
  ./node_modules/.bin
  ./vendor/bin
  "$HOME"/.rbenv/shims
  "$HOME"/bin
  "$HOME"/.composer/vendor/bin
  "$HOME"/.jetbrains/bin
  "$HOME"/.cargo/bin
  /usr/local/{bin,sbin}
  /usr/local/opt/grep/libexec/gnubin
  $path
)
